package minesweeper.events
{
	import flash.events.Event;
	
	/**
	 * MinefieldEvent class.
	 * Event class that contains the events used by many objects of the minesweeper.
	 * @author Jose Heriberto Lopez Ruiz, jlopez@joselopez.com.mx
	 **/
	public class MinefieldEvent extends Event
	{
		/**  NEW_GAME constant. **/
		public static const NEW_GAME:String = "NEW_GAME";
		/** GAME_OVER constant. **/
		public static const GAME_OVER:String = "GAME_OVER";
		/** WIN_GAME constant. **/
		public static const WIN_GAME:String = "WIN_GAME";
		/** START_GAME constant. **/
		public static const START_GAME:String = "START_GAME";
		/** SWEEPING constant. **/
		public static const SWEEPING:String = "SWEEPING";
		/** RESTING constant. **/
		public static const RESTING:String = "RESTING";
		/** TIME_UP constant. **/
		public static const TIME_UP:String = "TIME_UP";
		
		/**
		 * Constructor.
		 **/
		public function MinefieldEvent(type:String,bubbles:Boolean=false,cancelable:Boolean=false)
		{
			super(type, bubbles, cancelable);
		}
	}
}